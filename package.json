{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 9010",
    "clean": "rimraf .next .turbo node_modules/.cache .genkit",
    "rebuild": "rimraf node_modules package-lock.json && npm install",
    "test": "vitest run",
    "test:rules": "vitest run tests/rules --reporter=dot",
    "emulators": "firebase emulators:start --only firestore,storage,auth",
    "import:csv": "tsx -r dotenv/config src/tools/import/cli.ts",
    "import:generic": "node src/tools/import-generic-csv.mjs",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 9010",
    "analyze": "ANALYZE=true next build",
    "bnpl:reconcile": "node scripts/reconcile-bnpl.mjs",
    "bnpl:reconcile:dry": "node scripts/reconcile-bnpl.mjs --dry",
    "bnpl:recompute": "node scripts/recompute-bnpl-rollups.mjs",
    "alerts:backfill": "node scripts/alerts-backfill.mjs",
    "bnpl:check": "node scripts/check-bnpl-dualwrite.mjs",
    "bnpl:check:fix": "node scripts/check-bnpl-dualwrite.mjs --fix",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 -f node_modules/eslint-formatter-github",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "predev": "node scripts/free-port.mjs 9010 9002 || true",
    "typecheck": "tsc -p .",
    "preinstall": "mkdir -p /tmp/.npm-cache /tmp/.npm-tmp || true",
    "audit:client-imports": "node scripts/audit-client-imports.mjs",
    "audit:client-imports:fix": "node scripts/audit-client-imports.mjs --fix"
  },
