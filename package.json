    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.19.0",
    "@types/react": "^18",
