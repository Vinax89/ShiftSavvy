    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.8.0"
  },
  "overrides": {
    "undici": "^6.21.3"
  }
