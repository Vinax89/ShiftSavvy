    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sentry/nextjs": "^10.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.12",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "^8.6.0",
    "fast-xml-parser": "^4.4.0",
    "firebase": "^12.2.1",
